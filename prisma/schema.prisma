// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Yt_Playlist{
  id String @id @default(uuid()) 
  image String
  video String  
  title String
  @@map ("yt_playlists")
}
